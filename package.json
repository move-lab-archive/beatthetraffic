{
  "name": "beat-the-traffic",
  "version": "1.0.0",
  "scripts": {
    "build": "next build",
    "cfn:lint": "cfn-lint validate cloudformation.yaml",
    "cfn:update": "cfn-create-or-update --stack-name beatthetraffice-moovellab-com --template-body file://cloudformation.yaml --tags Key=Squad,Value=Lab Key=CostCenter,Value=7000 --region eu-west-1 --capabilities CAPABILITY_NAMED_IAM --wait",
    "deploy-prod": "now",
    "deploy": "now -n traffic-cam out",
    "dev": "node server/server-static-generation.js",
    "export": "next build && NODE_ENV=production next export",
    "now-build": "next build && NODE_ENV=production next export",
    "now-start": "NODE_ENV=production NOW_DEPLOY=true node server/server.js",
    "precommit": "npm run cfn:lint",
    "start-aws": "NODE_ENV=production MONGO_INSTANCE=ec2-54-246-254-133.eu-west-1.compute.amazonaws.com:27017/db node server/server.js",
    "start": "NODE_ENV=production node server/server-static-generation.js",
    "static": "serve out"
  },
  "dependencies": {
    "@turf/distance": "^5.1.5",
    "axios": "^0.17.1",
    "bezier-easing": "^2.0.3",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "express": "^4.16.2",
    "express-rate-limit": "^2.11.0",
    "geoip-lite": "^1.2.1",
    "gl-react": "2.3.1",
    "gl-react-dom": "3.13.0",
    "gsap": "^1.20.3",
    "howler": "^2.0.8",
    "immutable": "^3.8.2",
    "lodash.minby": "^4.6.0",
    "mongodb": "^3.0.2",
    "next": "^4.2.3",
    "next-redux-wrapper": "^1.3.5",
    "prop-types": "^15.6.0",
    "raf": "^3.4.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-ga": "^2.4.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "screenfull": "^3.3.2",
    "serve": "^6.4.8",
    "smoothscroll-polyfill": "^0.4.0"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "cfn-create-or-update": "^1.2.0",
    "cfn-lint": "^1.5.0",
    "eslint": "^4.16.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "prettier": "^1.10.2",
    "prettier-eslint": "^8.8.1"
  }
}
